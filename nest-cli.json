{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/monitoring_bot/src",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/monitoring_bot/tsconfig.app.json"
  },
  "projects": {
    "toogoodtogo": {
      "type": "library",
      "root": "libs/toogoodtogo",
      "entryFile": "index",
      "sourceRoot": "libs/toogoodtogo/src",
      "compilerOptions": {
        "tsConfigPath": "libs/toogoodtogo/tsconfig.lib.json"
      }
    },
    "cache": {
      "type": "library",
      "root": "libs/cache",
      "entryFile": "index",
      "sourceRoot": "libs/cache/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cache/tsconfig.lib.json"
      }
    },
    "monitoring_bot": {
      "type": "application",
      "root": "apps/monitoring_bot",
      "entryFile": "main",
      "sourceRoot": "apps/monitoring_bot/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monitoring_bot/tsconfig.app.json"
      }
    },
    "billing": {
      "type": "application",
      "root": "apps/billing",
      "entryFile": "main",
      "sourceRoot": "apps/billing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billing/tsconfig.app.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "queue": {
      "type": "library",
      "root": "libs/queue",
      "entryFile": "index",
      "sourceRoot": "libs/queue/src",
      "compilerOptions": {
        "tsConfigPath": "libs/queue/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/monitoring_bot"
}